	@Test
	public void mutipleObjectExtraction() {
		
		browsers("Chrome");
		driver.manage().timeouts().implicitlyWait(20,TimeUnit.SECONDS);//Implicit globle wait.
		driver.get("https://edition.cnn.com/");
		
		WebElement outBoxULTag=driver.findElement(By.xpath("//ul[@class='cn cn-list-xs cn--idx-0 cn-container_ED03BAA0-E1BF-D5E4-2FF0-E6CF17C3C0B4 cn--expandable cn--collapsed']"));
	
		List<WebElement> innerBoxlinks=outBoxULTag.findElements(By.tagName("a"));
		
		for(int i=0;i<innerBoxlinks.size();i++) {
			String url=innerBoxlinks.get(i).getAttribute("href");
			System.out.println(url);
			//*[@id="intl_homepage1-zone-2"]/div[5]/div/div[1]/ul/li[3]/article/div/div/h3/a/span[2]
			//*[@id="intl_homepage1-zone-2"]/div[5]/div/div[1]/ul/li[5]/article/div/div/h3/a/span[2]
		}
		
		List<WebElement> element =driver.findElements(By.xpath("//*[@id='intl_homepage1-zone-2']/div[5]/div/div[1]/ul/li/article/div/div/h3/a/span[2]"));
		//When we get pattern in that only li number is changing just pick that like above storre in webelement list and get text.
		for(int i=0;i<element.size();i++) {
			String namText=innerBoxlinks.get(i).getText();
			System.out.println(namText);
			//*[@id="intl_homepage1-zone-2"]/div[5]/div/div[1]/ul/li[3]/article/div/div/h3/a/span[2]
			//*[@id="intl_homepage1-zone-2"]/div[5]/div/div[1]/ul/li[5]/article/div/div/h3/a/span[2]
		}
		
	}
}
	
