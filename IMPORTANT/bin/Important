		==================================Window Handle=====================================
		
		@Test
	public void windowHandleTest() {
		
		browsers("Chrome");
		driver.manage().window().maximize();
		driver.get("https://www.naukri.com");
		
		//Lets see how many windows are there on page
		//set is un Ordered set but below set.iterator(); method will give you the ordered set.
		Set<String> set=driver.getWindowHandles();
		
		System.out.println("Number of windows ="+set.size());
		
		
		Iterator<String> itr=set.iterator();//it will give you the ids of windows  
		
		String mainWindowID=itr.next();//it will give the main window id
		
		String secondWindowID=itr.next();//it will give the second window id
		String thirdWindowID=itr.next();//it will give the third window id
		
		System.out.println("mainWindowID="+mainWindowID);
		System.out.println("secondWindowID="+secondWindowID);
		System.out.println("thirdWindowID="+thirdWindowID);
		
		
		driver.switchTo().window(secondWindowID);
		//driver.close();//this will close the pop up so we can understand what is second and thired window.
		
		
		/*driver.switchTo().newWindow(WindowType.TAB);//Its a feature of Selenium 4 we can open window in new tab.
		
		driver.manage().window().maximize();
		*/
		
		driver.switchTo().window(thirdWindowID);
		
		
		//driver.quit() >> it closes all windows and selenium session ; and driver.close();>>> it closes under focused window 
		
		
		/*driver.switchTo().window(mainWindowID); //when it will not work below will work.
		driver.switchTo().defaultContent();
		*/
	}
	
		
		
		
		==================================Collection API Set=========================
		//Set holds unique elements > mean no duplicate element
		//no final size >> it grows dynamicly means if we sysout before size would show 0 but if we pul last , it will give accordingly.
		// no indexing like List >> means when we try to print it, it prints in any order.
		
		
		Set<String> set= new HashSet <String>();
		
		System.out.println("set-Size getting on top="+set.size());
		set.add("Sandeep");
		set.add("Pradeep");
		set.add("Debashish");
		set.add("Akhilesh");
		
		set.add("Akhilesh");//Set holds unique elements > mean no duplicate element, size wont change
		
		System.out.println("set-Size getting on bottom="+set.size());  // it grows dynamicly means if we sysout before size would show 0 but if we pul last , it will give accordingly.
		
		Iterator<String> itr=set.iterator();
		
		//We can use any loop but while is best to use.
		
		/*for(int i=0;i<set.size();i++) {
			System.out.println("Printing from For loop"+itr.next());
		
		
		}*/
		
		
		while(itr.hasNext()) {
			System.out.println("Printing from While loop"+itr.next());
		}
		
		
		
		
	================================Handling Alert Pop ups==============================	
	@Test
	public void windowHandleTest() {
		
		browsers("Chrome");
		driver.manage().window().maximize();
		driver.get("https://mail.rediff.com/cgi-bin/login.cgi");
		
		driver.findElement(By.xpath("//input[@id='login1']")).sendKeys("abc");
		driver.findElement(By.xpath("//input[@title='Sign in']")).click();
		
		//some time alert comes little late so in that case we can use Webdrover wait
		WebDriverWait explicitwait= new WebDriverWait(driver, Duration.ofSeconds(20));
		explicitwait.until(ExpectedConditions.alertIsPresent());
		
		
		
		Alert alert=driver.switchTo().alert();
		
		System.out.println("Text available on alert Pop up="+alert.getText());
		
		alert.accept();//clicks on OK button
		
	//	alert.dismiss();//cancel button if available on pop up some time
	
		
		// if you want to bring back control to main window
		driver.switchTo().defaultContent();
		
	}
		
		
		
		
		
		
		====================================Handling webTable=====================
		@Test
	public void webTableTest() {
		
		//Every Web table we get like
		//<table class='datatable'> 
		//<tr>== row of table and td tag is column of each row
		//<table class='datatable'> /tbody/tr[3]/td[4] it means 3rd row and 4rth column.
		//*[@id='leftcontainer']/table/tbody/tr[1]/td *** if we remove td number it will become genric for that row.
		//*[@id='leftcontainer']/table/tbody/tr/td[4]***(If i remove tr number it will represent whole 4rth column)
		//*[@id='leftcontainer']/table/tbody/tr ********(If i put only tr it will represent whole rows of table)
		
		
		//now in this web table we want current price of any given company name if com name is dynamic.
		
		browsers("Chrome");
		driver.manage().window().maximize();
		driver.get("https://money.rediff.com/gainers/bse/daily/groupa?src=gain_lose");
		
		
		//*[@id='leftcontainer']/table/tbody/tr/td[1] >> here it will give first row and first column
		List<WebElement> listOfCompany_Name=driver.findElements(By.xpath("//*[@id='leftcontainer']/table/tbody/tr/td[1]"));
		System.out.println("Size of or number of Comapany names="+listOfCompany_Name.size());
		
		List<WebElement> CurrentPriceColumn=driver.findElements(By.xpath("//*[@id='leftcontainer']/table/tbody/tr/td[4]"));
		System.out.println("Size of or number of Comapany' current price names="+CurrentPriceColumn.size());
		
		//both size should be matched.
		
		String reqCompany_NameToPrint="Mahindra Lifespace D";
		for(int i=0;i<listOfCompany_Name.size();i++) {
			
			if(reqCompany_NameToPrint.equals(listOfCompany_Name.get(i).getText())) {
				
				System.out.println("Comapany Name="+listOfCompany_Name.get(i).getText()+"-------And Price of this company name is="+CurrentPriceColumn.get(i).getText());
				break;
			}
			
		
		}
		=============================WebTable Method==========================
		public int getTablerowData(String data) {
		
		List<WebElement> Rows=driver.findElements(By.xpath("//*[@id='leftcontainer']/table/tbody/tr"));
		
		for (int i=0;i<Rows.size();i++) {
			
			WebElement RowElement=Rows.get(i);
			
			List<WebElement> AllCells=RowElement.findElements(By.tagName("td"));
			
			for(int j=0;j<AllCells.size();j++) {
				
				//System.out.println(AllCells.get(j).getText());
				
				
				if(data.equals(AllCells.get(j).getText())) {

					return j+1;
				}
			}
			
			
		}
		return -1;
		
		
		
		
		
	
		
		
				}
			
		
		
		method calling:
			String CompanyName="Indus Towers";
		int number=getTablerowData(CompanyName);
		Assert.assertTrue(number !=-1, "Company name not found");
	
		String price=driver.findElement(By.xpath("//*[@id='leftcontainer']/table/tbody/tr["+number+"]/td[4]")).getText();
		System.out.println("CompanyName="+CompanyName+" And price="+price);
		
		
		
		
		
		
		=============================waitForPageLoad Java Script function==============================================
		public void waitForPageToLoad() throws InterruptedException{
		JavascriptExecutor js = (JavascriptExecutor)driver;
		int i=0;
		// page to load 100% -- wait for max 20 secs
		while(i!=10){
			String state = (String)js.executeScript("return document.readyState;");
			System.out.println(state);// complete, loading, interactive
	
			if(state.equals("complete"))
				break;
			else
				Thread.sleep(2000);
	
			i++;
		}
		// check for jquery/ajax status
		
		i=0;
		while(i!=10){

			Long d= (Long) js.executeScript("return jQuery.active;");
			System.out.println(d);//0,1
			if(d.longValue() == 0 )
				break;
			else
				Thread.sleep(2000);
			i++;

		}
		
	}
		
		
		
		
