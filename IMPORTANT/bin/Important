	============================To get the Response code of any url============================
			public static void main(String[] args) {
		
		String url="https://edition.cnn.com/business";
		
		
		int resp_code=0;
		try {
		resp_code = Request.Get(url).execute().returnResponse().getStatusLine()//org.apache.http.client.fluent.Request
		       .getStatusCode();
		System.out.println("Respose code for URL "+ url +" is -> "+ resp_code);
		       
		} catch (Exception e) {
		// TODO Auto-generated catch block
		e.printStackTrace();
		}

	}
