		@Test
	public void handle_JavaScriptTest() {
		
		
		
		browsers("Chrome");
		
		
		
	
		//driver.get("https://money.rediff.com/gainers/bse/daily/groupa?src=gain_lose");
		
		JavascriptExecutor javascriptExecutor= (JavascriptExecutor) driver; //Object casting so we can use java script. 
		
		javascriptExecutor.executeScript("window.location='https://www.linkedin.com/login'"); //window.location is javascript command and works as driver.get() method
		
		driver.manage().window().maximize();
		
		javascriptExecutor.executeScript("document.getElementById('username').value='Hello'");// its like driver.send keys
		
		//Browser understands only java script command , when we type any command in selenium it gets converted in java script.
		
		javascriptExecutor.executeScript("window.scrollTo(0,100)");//This is for sclolling as selenium usually dont have scroll cammand.
		
		
}
		
		
		
		
		================================Actions ,mouse movement slider used scroll bottom used===mouse hover============================
		
		
		@Test
	public void handle_JavaScriptTest() throws InterruptedException {
		
		// Actions class is used for the gestures Like:
		// moving mouse or mouse howver
		//drag drop element
		//By moving the mouse if we want to click somewhere
		
		
		browsers("Chrome");
		driver.manage().window().maximize();
		driver.get("https://www.americangolf.co.uk/");
		
		Actions actions= new Actions(driver); //creating Actions object
		
		WebElement  elementGolfClubs=driver.findElement(By.xpath("//*[@id='header-navigation']/div[1]/ul/li[2]/a"));
		
		actions.moveToElement(elementGolfClubs).build().perform();// Will do mouse hover, build().perform() is must write every Actions class otherwise it will not work
		
		
		//Some time it takes time to appear the element so we use Explicit wait:
		WebDriverWait webWait= new WebDriverWait(driver, Duration.ofSeconds(10));
		webWait.until(ExpectedConditions.visibilityOfElementLocated(By.xpath("//*[@id='CLUBS_1']/ul/li[3]/ul/li/a/span")));
		
		String putters_Link=driver.findElement(By.xpath("//*[@id='CLUBS_1']/ul/li[3]/ul/li/a/span")).getText();
		System.out.println("putters_Link="+putters_Link);
		driver.findElement(By.xpath("//*[@id='CLUBS_1']/ul/li[3]/ul/li/a/span")).click();
		
		waitForPageToLoad();
		//This is the best way if we get same class two elements just find which one we want like it highlight and use below and get that.
	 WebElement SecondndElement_Slider=driver.findElements(By.xpath("//div[@class='noUi-handle noUi-handle-lower']")).get(1);
		
		int y=SecondndElement_Slider.getLocation().y; //here we are getting the coordinate of second element and save in int y
		waitForPageToLoad();
		JavascriptExecutor javascriptExecutor= (JavascriptExecutor) driver;
		javascriptExecutor.executeScript("window.scrollTo(0,"+(y-80)+")");//as it scrolled to much so subtracting 80
		
		actions.clickAndHold(SecondndElement_Slider).moveByOffset(100, 0).release().build().perform();//it is for click any element hold and moveByOffset gives coordinate and release will release hold of mouse.
		
		
		
		
		
		
		
		
		
		
		=============================Select one in Two highligheted element =====used above===============
		//This is the best way if we get same class two elements just find which one we want like it highlight and use below and get that.
	 WebElement SecondndElement_Slider=driver.findElements(By.xpath("//div[@class='noUi-handle noUi-handle-lower']")).get(1);
		
		
		
		
		
		
		====================================Random Class============Actions class Actions ,mouse movement===========================
		
		@Test
	public void handle_JavaScriptTest() throws InterruptedException {
		
		// Actions class is used for the gestures Like:
		// moving mouse or mouse howver
		//drag drop element
		//By moving the mouse if we want to click somewhere
		
		
		browsers("Chrome");
		driver.manage().window().maximize();
		driver.get("https://www.americangolf.co.uk/");
		
		Actions actions= new Actions(driver);
		
		WebElement  elementGolfClubs=driver.findElement(By.xpath("//*[@id='header-navigation']/div[1]/ul/li[2]/a"));
		
		actions.moveToElement(elementGolfClubs).build().perform();// Will do mouse hover, build().perform() is must write every Actions class otherwise it will not work
		
		
		//Some time it takes time to appear the element so we use Explicit wait:
		WebDriverWait webWait= new WebDriverWait(driver, Duration.ofSeconds(10));
		webWait.until(ExpectedConditions.visibilityOfElementLocated(By.xpath("//*[@id='CLUBS_1']/ul/li[3]/ul/li/a/span")));
		
		List<WebElement> allLinks_underGolfClubs=driver.findElements(By.xpath("//div[@id='CLUBS_1']/descendant::a"));
		
		for(int i=0;i<allLinks_underGolfClubs.size();i++) {
			
			System.out.println(allLinks_underGolfClubs.get(i).getText());
		}
		
		//Use of Randome class
		Random random= new Random();
		int RandomeNomberOfGolfmenue=random.nextInt(allLinks_underGolfClubs.size());
		
		allLinks_underGolfClubs.get(RandomeNomberOfGolfmenue).click(); //clicking on any randome number link
		
		
		
	 ****	When we use single command to do we can use 
		actions.moveToElement(elementGolfClubs).perform(); we can use perform only
		
		but when we use two commands in sigle line like:
		suppose there is a search box where we have to get  presses tab key and enter key by keyboard here we use build and perform both commands. 
		actions.sendkeys(keys.Tab).sendkeys(keys.ENTER).build().perform();
		
		
		
		
		
		
		
	=====================================Handling IFrames===========================================================
		@Test
	public void Handling_FramesTest() throws InterruptedException {
		
		browsers("Chrome");
		driver.manage().window().maximize();
		driver.get("https://paytm.com");
		
		WebDriverWait webWait= new WebDriverWait(driver, Duration.ofSeconds(10));
		webWait.until(ExpectedConditions.visibilityOfElementLocated(By.xpath("//*[@id='app']/div/div[2]/div[2]/div[3]/div[4]/div")));
		
		driver.findElement(By.xpath("//*[@id='app']/div/div[2]/div[2]/div[3]/div[4]/div")).click();
		
		
		
		//Few important points about "Frames"
		/*Because it is a Frame
		 * Frame is a kind of a page which embeded somewhere inside the page.
		 * How to detect it is a frame just open mozilla which is very good for frame capabilities and open site
		 * if we do wright click main site in chrome, "This frame" option will not show
		 * But on pop up of Fram like paytm scan pop up if we wright click "This Frame" option comes up in mozilla.
		 - There may be multiple of frames in page so can we detect:
		 - Ask devloper and other option is to do with selenium.
		 
		*/
		
		
		List<WebElement> Frames=driver.findElements(By.tagName("iframe"));
		
		System.out.println("Total no of frams available on page are="+Frames.size());
		driver.switchTo().frame(0);//as we have only one frame
		
		WebDriverWait webWait1= new WebDriverWait(driver, Duration.ofSeconds(10));
		webWait1.until(ExpectedConditions.visibilityOfElementLocated(By.xpath("//*[@id='main-container']/div/div/div/div/div/div[2]/qr-code-login/div/div[2]/div")));
		
		
		String text=driver.findElement(By.xpath("//*[@id='main-container']/div/div/div/div/div/div[2]/qr-code-login/div/div[2]/div")).getText();
		System.out.println("text="+text);
		
		driver.switchTo().defaultContent();//It uses to come back to main window.
		
		
		// *****Now suppose there are 5 iframes available how we can differenciate which one we want to deal with.
		
		for(int i=0;i<Frames.size(); i++) {
			
			//switching to frames one by one
			driver.switchTo().frame(i);
			
			//Check the presense of element if element present then break the loop.
			int size=driver.findElements(By.xpath("//*[@id='main-container']/div/div/div/div/div/div[2]/qr-code-login/div/div[2]/div")).size();
			
			if(size==0) { //size 0 means element not found 
				
				driver.switchTo().defaultContent();//It uses to come back to main window.
			} 
			else
				break; //if size is not 0 then means we found the element and break the loop and start interecting with element 
			
		}
		
		String text1=driver.findElement(By.xpath("//*[@id='main-container']/div/div/div/div/div/div[2]/qr-code-login/div/div[2]/div")).getText();
		System.out.println("Just doing again if we will have more than 1 frames ="+text1);
		
		
		driver.switchTo().defaultContent();//and once we done just we get back to main window.
}
		
		
		
		
